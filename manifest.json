{
  "manifest_version": 2,
  "name": "Apollo Client Chrome Developer Tools",
  "short_name": "Apollo DevTools",
  "version": "3.0.0",
  "description": "GraphQL debugging tools for Apollo Client in the Chrome developer console.",
  "icons": {
    "16": "app/icons/16.png",
    "48": "app/icons/48.png",
    "128": "app/icons/128.png"
  },
  "permissions": ["*://*/*"],
  "devtools_page": "environment/index.html",
  "web_accessible_resources": ["injected/injected.js"]
}
