{
  "name": "apollo-client-devtools",
  "description": "Developer tools for Apollo Client, with Studio Explorer integration, cache inspection, and query watching.",
  "license": "MIT",
  "version": "4.2.3",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/apollographql/apollo-client-devtools.git"
  },
  "scripts": {
    "install:client": "npm install --prefix development/client",
    "install:server": "npm install --prefix development/server",
    "install:dev": "run-p install:client install:server",
    "start:client": "npm start --prefix development/client",
    "start:server": "npm start --prefix development/server",
    "start:dev": "run-p start:client start:server",
    "dist": "tsx build.ts",
    "build": "npm run clean && webpack --env NODE_ENV=production --progress",
    "build:dev": "npm run clean && webpack --env NODE_ENV=development --progress",
    "clean": "rimraf build && mkdir build && rimraf *.zip",
    "changeset-publish": "npm run build && changeset publish",
    "changeset-check": "changeset status --verbose --since=origin/main",
    "changeset-version": "changeset version && npm i && npm run prepdist:changesets",
    "prepdist:changesets": "tsx prepareChangesetsRelease.ts && prettier src/extension/manifest.json --write",
    "chrome": "npm run build:dev -- --watch --env TARGET=chromium",
    "firefox": "npm run build:dev -- --watch",
    "format": "prettier --write .",
    "check:format": "prettier --check .",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "test": "jest",
    "type-check": "tsc --noEmit",
    "test:circleci": "jest --maxWorkers=2",
    "test:watch": "jest --watch",
    "codegen": "graphql-codegen --config codegen.ts"
  },
  "dependencies": {
    "@apollo/client": "^3.7.10",
    "@apollo/space-kit": "^9.11.0",
    "@emotion/react": "^11.11.1",
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "@radix-ui/react-tabs": "^1.0.2",
    "graphql": "^16.0.0",
    "graphql-tag": "^2.11.0",
    "polished": "^4.1.1",
    "prism-react-renderer": "^2.0.0",
    "react": "^18.2.0",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "^18.2.0",
    "react-json-tree": "^0.18.0",
    "react-resizable-panels": "^0.0.55",
    "react-syntax-highlighter": "^15.5.0",
    "zen-observable": "^0.10.0"
  },
  "devDependencies": {
    "@changesets/changelog-github": "0.4.8",
    "@changesets/cli": "2.26.2",
    "@emotion/core": "10.3.1",
    "@emotion/eslint-plugin": "11.11.0",
    "@emotion/jest": "11.11.0",
    "@graphql-codegen/cli": "5.0.0",
    "@graphql-codegen/typescript": "4.0.1",
    "@graphql-codegen/typescript-operations": "4.0.1",
    "@graphql-tools/mock": "9.0.0",
    "@graphql-tools/schema": "10.0.0",
    "@parcel/watcher": "2.3.0",
    "@testing-library/jest-dom": "6.1.4",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.5.1",
    "@total-typescript/ts-reset": "0.5.1",
    "@types/archiver": "5.3.4",
    "@types/chrome": "0.0.248",
    "@types/jest": "29.5.6",
    "@types/react": "18.2.31",
    "@types/react-copy-to-clipboard": "5.0.6",
    "@types/react-dom": "18.2.14",
    "@types/webextension-polyfill": "0.10.5",
    "@types/zen-observable": "0.8.5",
    "@typescript-eslint/eslint-plugin": "6.8.0",
    "@typescript-eslint/parser": "6.8.0",
    "archiver": "6.0.1",
    "autoprefixer": "10.4.16",
    "copy-webpack-plugin": "11.0.0",
    "css-loader": "6.8.1",
    "eslint": "8.52.0",
    "eslint-plugin-jest-dom": "5.1.0",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-testing-library": "6.1.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jest-matchmedia-mock": "1.1.0",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.31",
    "postcss-loader": "7.3.3",
    "prettier": "3.0.3",
    "resize-observer-polyfill": "1.5.1",
    "rimraf": "5.0.5",
    "style-loader": "3.3.3",
    "tailwindcss": "3.3.3",
    "terser-webpack-plugin": "5.3.9",
    "ts-jest": "29.1.1",
    "ts-loader": "9.5.0",
    "tsx": "3.14.0",
    "typescript": "5.2.2",
    "web-ext-plugin": "2.8.1",
    "webextension-polyfill": "0.10.0",
    "webpack": "5.89.0",
    "webpack-cli": "5.1.4"
  }
}
