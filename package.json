{
  "name": "apollo-client-devtools",
  "description": "Developer tools for Apollo Client, with Studio Explorer integration, cache inspection, and query watching.",
  "license": "MIT",
  "version": "4.1.6",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/apollographql/apollo-client-devtools.git"
  },
  "scripts": {
    "install:client": "npm install --prefix development/client",
    "install:server": "npm install --prefix development/server",
    "install:dev": "run-p install:client install:server",
    "start:client": "npm start --prefix development/client",
    "start:server": "npm start --prefix development/server",
    "start:dev": "run-p start:client start:server",
    "build": "npm run clean && webpack --env NODE_ENV=development --progress",
    "build:prod": "npm run clean && webpack --env NODE_ENV=production --progress",
    "check:format": "prettier --check .",
    "clean": "rimraf build && mkdir build && rimraf dist && mkdir dist",
    "changeset-publish": "npm run build:prod && changeset publish",
    "changeset-check": "changeset status --verbose --since=origin/main",
    "changeset-version": "changeset version && npm i && npm run prepdist:changesets",
    "chrome": "npm run build -- --watch --env TARGET=chromium",
    "firefox": "npm run build -- --watch",
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "prepublishOnly": "npm run build",
    "prepdist:changesets": "ts-node-script prepareChangesetsRelease.ts && prettier src/extension/manifest.json --write",
    "prezip": "npm run build:prod",
    "test": "jest",
    "type-check": "tsc --noEmit",
    "test:circleci": "jest --maxWorkers=2",
    "test:watch": "jest --watch",
    "zip": "npm run prezip && npm run zip:chrome && npm run zip:firefox",
    "zip:chrome": "zip -r -FS dist/chrome.zip build",
    "zip:firefox": "web-ext build --source-dir build --artifacts-dir dist --overwrite-dest",
    "codegen": "graphql-codegen --config codegen.ts"
  },
  "dependencies": {
    "@apollo/client": "^3.7.10",
    "@apollo/space-kit": "^9.11.0",
    "@emotion/react": "^11.11.1",
    "@radix-ui/react-tabs": "^1.0.2",
    "graphql": "^16.0.0",
    "graphql-tag": "^2.11.0",
    "polished": "^4.1.1",
    "prism-react-renderer": "^2.0.0",
    "react": "^18.2.0",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "^18.2.0",
    "react-json-tree": "^0.18.0",
    "react-resizable-panels": "^0.0.55",
    "react-syntax-highlighter": "^15.5.0",
    "zen-observable": "^0.10.0"
  },
  "devDependencies": {
    "@changesets/changelog-github": "0.4.8",
    "@changesets/cli": "2.26.2",
    "@emotion/core": "10.3.1",
    "@emotion/eslint-plugin": "^11.11.0",
    "@emotion/jest": "11.11.0",
    "@graphql-codegen/cli": "5.0.0",
    "@graphql-codegen/typescript": "4.0.1",
    "@graphql-codegen/typescript-operations": "4.0.1",
    "@graphql-tools/mock": "9.0.0",
    "@graphql-tools/schema": "10.0.0",
    "@parcel/watcher": "2.3.0",
    "@testing-library/jest-dom": "6.1.2",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "@total-typescript/ts-reset": "0.5.1",
    "@types/chrome": "0.0.244",
    "@types/jest": "29.5.4",
    "@types/react": "18.2.21",
    "@types/react-copy-to-clipboard": "^5.0.4",
    "@types/react-dom": "18.2.7",
    "@types/webextension-polyfill": "0.10.2",
    "@types/zen-observable": "^0.8.4",
    "@typescript-eslint/eslint-plugin": "6.5.0",
    "@typescript-eslint/parser": "6.5.0",
    "copy-webpack-plugin": "11.0.0",
    "css-loader": "5.2.7",
    "eslint": "8.48.0",
    "eslint-plugin-jest-dom": "5.1.0",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-testing-library": "6.0.1",
    "jest": "29.6.4",
    "jest-environment-jsdom": "29.6.4",
    "jest-matchmedia-mock": "1.1.0",
    "npm-run-all": "4.1.5",
    "prettier": "3.0.3",
    "rimraf": "5.0.1",
    "style-loader": "2.0.0",
    "terser-webpack-plugin": "5.3.9",
    "ts-jest": "29.1.1",
    "ts-loader": "8.4.0",
    "ts-node": "10.9.1",
    "typescript": "4.9.5",
    "web-ext": "7.6.2",
    "web-ext-plugin": "2.8.1",
    "webextension-polyfill": "0.10.0",
    "webpack": "5.88.2",
    "webpack-cli": "5.1.4"
  }
}
