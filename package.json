{
  "name": "apollo-client-devtools",
  "description": "Chrome Developer tools for Apollo Client, with GraphiQL integration, cache inspection, and query watching.",
  "license": "MIT",
  "version": "3.0.0-alpha.0",
  "scripts": {
    "build": "",
    "dev": "NODE_ENV=development webpack --mode development --config webpack.js --watch",
    "prettier": "prettier --write --config=./.prettierrc \"{,!(node_modules|dist)/**/}*.{ts*,js*,json,md}\"",
    "prepublish": "npm run build",
    "clean": "rimraf build/*"
  },
  "dependencies": {
    "@reach/tabs": "^0.6.2",
    "classnames": "^2.2.6",
    "keymirror": "^0.1.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-react": "^7.7.0",
    "@types/chrome": "0.0.91",
    "babel-loader": "^8.0.6",
    "copy-webpack-plugin": "^5.0.5",
    "css-loader": "^3.2.0",
    "file-loader": "^4.2.0",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.0",
    "style-loader": "^1.0.0",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.3",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run prettier",
      "git add"
    ]
  }
}
